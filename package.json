{
  "name": "algorithms-utils",
  "version": "1.0.0",
  "description": "算法与 TypeScript 实现",
  "main": "dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf dist types && gulp",
    "log": "rimraf CHANGELOG.md && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziyi2/algorithms.git"
  },
  "keywords": [
    "algorithms",
    "typescript"
  ],
  "author": "ziyi2",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ziyi2/algorithms/issues"
  },
  "homepage": "https://github.com/ziyi2/algorithms#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "conventional-changelog-cli": "^2.0.34",
    "cz-customizable": "^6.2.1",
    "eslint": "^7.4.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "merge2": "^1.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
